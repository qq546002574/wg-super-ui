{
  "name": "wg-component-library",
  "version": "2.0.23-ajt",
  "description": "组件库",
  "keywords": [
    "vue",
    "component-library"
  ],
  "author": "duanweiye",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://172.26.0.18/frontend/util/wg-super-ui.git"
  },
  "publishConfig": {
    "registry": "http://172.26.0.19:8081/repository/npm-repo"
  },
  "main": "dist/lib/wg-component-library.umd.min.js",
  "scripts": {
    "demo:dev": "vue-cli-service serve --open demo/main.js",
    "demo:build": "vue-cli-service build demo/main.js",
    "docs:dev": "vuepress dev docs --open",
    "docs:build": "vuepress build docs",
    "lib:build": "vue-cli-service build --target lib src/index.js",
    "lib:build:report": "vue-cli-service build --report --target lib src/index.js",
    "lib:publish": "npm run lib:build && npm publish",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@tiptap/extension-bold": "^2.1.0",
    "@tiptap/extension-color": "^2.1.0",
    "@tiptap/extension-highlight": "^2.9.1",
    "@tiptap/extension-image": "^2.1.0",
    "@tiptap/extension-link": "^2.1.0",
    "@tiptap/extension-placeholder": "^2.1.0",
    "@tiptap/extension-table": "^2.9.1",
    "@tiptap/extension-table-cell": "^2.9.1",
    "@tiptap/extension-table-header": "^2.9.1",
    "@tiptap/extension-table-row": "^2.9.1",
    "@tiptap/extension-task-item": "^2.9.1",
    "@tiptap/extension-task-list": "^2.9.1",
    "@tiptap/extension-text-align": "^2.1.0",
    "@tiptap/extension-text-style": "^2.1.0",
    "@tiptap/extension-typography": "^2.9.1",
    "@tiptap/starter-kit": "^2.1.0",
    "@tiptap/vue-2": "^2.1.0",
    "axios": "^0.18.1",
    "codemirror": "^5.65.2",
    "dayjs": "^1.11.7",
    "docx": "^9.5.1",
    "dom-zindex": "^1.0.6",
    "drawio-embed": "^1.0.2",
    "element-ui": "^2.15.13",
    "html-to-docx": "^1.8.0",
    "jszip": "^3.10.1",
    "keyboardjs": "^2.6.4",
    "mxgraph": "^4.2.2",
    "quill-image-resize-module": "^3.0.0",
    "quill-image-resize-module--fix-imports-error": "^3.0.0",
    "sortablejs": "^1.15.0",
    "vue": "^2.5.22",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vxe-table": "3.4.15",
    "vxe-table-plugin-element": "1.11.4",
    "xe-utils": "^3.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vuepress/plugin-back-to-top": "^1.0.0-alpha.42",
    "@vuepress/plugin-pwa": "^1.0.0-alpha.43",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.36.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less-loader": "^6.0.0",
    "lint-staged": "^8.1.0",
    "lodash.merge": "^4.6.2",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "^1.78.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.22",
    "vuepress": "^1.0.0-alpha.47",
    "yorkie": "^2.0.0"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "browser": false,
      "node": false
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {
      "vue/require-default-prop": false,
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 3
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "dist",
    "*"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {},
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
